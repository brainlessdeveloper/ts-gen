{
  "author": "Fausto Núñez Alberro <fausto.nunez@mailbox.org>",
  "description": "Generate jsverify.Arbitrary/io-ts/Runtypes from TypeScript types",
  "engines": {
    "yarn": "none - please use npm"
  },
  "engineStrict": true,
  "keywords": [
    "TypeScript",
    "generation",
    "jsverify",
    "io-ts",
    "runtypes"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.js",
  "name": "ts-gen",
  "repository": {
    "type": "git",
    "url": "https://github.com/brainlessdeveloper/ts-gen"
  },
  "version": "0.1.0",
  "scripts": {
    "build": "tsc",
    "ci": "npm-run-all build format:ci lint test:ci",
    "format": "prettier --write './src/**/*.ts' './test/**/*.ts'",
    "format:ci": "npm run format && git diff --exit-code",
    "lint": "tslint --exclude '**/*.js' --project .",
    "test": "jest --coverage --coverageReporters=lcov --watch",
    "test:ci": "jest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {},
  "peerDependencies": {
    "typescript": ">=2.1.0 || >=2.1.0-dev || >=2.2.0-dev || >=2.3.0-dev || >=2.4.0-dev || >=2.5.0-dev || >=2.6.0-dev || >=2.7.0-dev || >=2.8.0-dev || >=2.9.0-dev"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/node": "^11.11.3",
    "jest": "^24.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "ts-jest": "^24.0.0",
    "tslint": "^5.14.0",
    "typescript": "^3.3.3"
  }
}
